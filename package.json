{
  "name": "fact-digi",
  "description": "Fact Digi is a next-gen company offering services in Business Intelligence, AI, Data Solutions, and Web & Mobile Application Development.",
  "author": {
    "name": "Fact Digi",
    "email": "support@factdigi.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/diwashbhattarai999/fact-digi.git"
  },
  "homepage": ".",
  "license": "MIT",
  "keywords": [
    "react",
    "typescript",
    "landing-page",
    "responsive-design",
    "generative AI",
    "business-intelligence",
    "artificial-intelligence",
    "data-solutions",
    "web-development"
  ],
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "commit": "git cz",
    "prepare": "husky",
    "docker:build": "docker build -t fact-digi .",
    "docker:run": "docker run -p 3000:3000 fact-digi",
    "docker:stop": "docker stop $(docker ps -a -q)"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@hookform/resolvers": "^3.9.1",
    "@radix-ui/react-accordion": "^1.2.2",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-navigation-menu": "^1.2.3",
    "@radix-ui/react-slot": "^1.1.1",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query": "^5.62.8",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "embla-carousel": "^8.5.1",
    "embla-carousel-auto-scroll": "^8.5.1",
    "embla-carousel-autoplay": "^8.5.1",
    "embla-carousel-react": "^8.5.1",
    "framer-motion": "^11.15.0",
    "libphonenumber-js": "^1.11.18",
    "lucide-react": "^0.468.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.54.1",
    "react-markdown": "^9.0.3",
    "react-router": "^7.0.2",
    "react-router-hash-link": "^2.4.3",
    "sonner": "^1.7.1",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-bg-patterns": "^0.3.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/types": "^19.5.0",
    "@eslint/compat": "^1.2.0",
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-hash-link": "^2.4.9",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "@vitejs/plugin-react": "^4.3.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.19",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.10.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.38",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.8.0",
    "vite": "^6.0.9"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "pnpm run lint && pnpm run format"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "pnpm run lint",
      "pnpm run format"
    ]
  }
}
